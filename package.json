{
  "name": "baekjoonhub",
  "version": "2.0.0",
  "description": "Chrome extension that automatically pushes your code to GitHub from competitive programming platforms",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "webpack --watch --config webpack.config.js",
    "package": "npm run build && npm run zip",
    "clean": "rm -rf dist packages node_modules",
    "lint": "eslint src/**/*.js",
    "test": "echo \"No tests implemented yet\"",
    "zip": "node scripts/create-zip.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "keywords": [
    "chrome-extension",
    "baekjoonhub",
    "competitive-programming",
    "github-integration"
  ],
  "author": "flaxinger",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BaekjoonHub/BaekjoonHub.git"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "js-sha1": "^0.6.0",
    "jszip": "^3.7.1",
    "safe-template-parser": "^1.0.0"
  }
}
